import java.io.IOException;
import java.nio.file.Files;
import java.nio.file.Path;
import java.nio.file.Paths;
import java.util.*;
import java.util.stream.Stream;

//TIP To <b>Run</b> code, press <shortcut actionId="Run"/> or
// click the <icon src="AllIcons.Actions.Execute"/> icon in the gutter.
public class Main {

    public static void main(String[] args) throws IOException {
       int summationsCount =  printSummations(100);

        System.out.println("count : " + summationsCount);
    }

    static int  printSummations(int n){
        List<Integer> summationList = new ArrayList<>();

        int count = 1;

        summationList.add(n-1);

        summationList.add(1);

        System.out.println(summationList);

        while(summationList.get(0)>1){
            int lastIndex= summationList.size()-1;

            if(summationList.get(lastIndex)==1){
                int partialSum = 1;
                int onesIndex = lastIndex-1;
                while(summationList.get(onesIndex)==1){
                    partialSum+=1;
                    onesIndex--;
                }

                int firstNonOneFromLeft = summationList.get(onesIndex);

                partialSum+=(firstNonOneFromLeft);

                int quot = partialSum/(firstNonOneFromLeft-1);

                int rem = partialSum%(firstNonOneFromLeft-1);

                summationList =  new ArrayList<Integer>(summationList.subList(0,onesIndex));

                for(int i=0; i<quot; i++){
                    summationList.add(firstNonOneFromLeft-1);
                }

                if(rem>0){
                    summationList.add(rem);
                }

            }else{
                summationList.set(lastIndex, summationList.get(lastIndex) -1);
                summationList.add(1);
            }

            count++;
            //System.out.println(summationList);
        }
        return count;
    }

}
